    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/dist/app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TEst",
            "program": "${workspaceFolder}/node_modules/.bin/jasmine",
            "args": [
                "dist/spec/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}//dist/app.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
